@model AutoType
@{
    ViewData["Title"] = "Добавление вида автомобиля";
}
<h2>Добавление вида автомобиля</h2>
<form asp-action="CreateAutoType" asp-controller="AutoTypes">
    <div class="form-group">
        <label asp-for="Name"></label>
        <input type="text" asp-for="Name" class="form-control"/>
    </div>
    <br />
    <div class="form-group">
        <label asp-for="Description"></label>
        <input type="text" asp-for="Description" class="form-control" />
    </div>
    <br />
    <div class="form-group">
        <input type="submit" value="Добавить вид автомобиля" class="btn btn-primary" />
    </div>
</form>